    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "baseUrl": "./",
    "module": "ESNext",
